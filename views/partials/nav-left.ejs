<!-- Admin View -->
<!-- Admin Left Sidebar -->

        <!--Left side column. contains the logo and sidebar -->
            <aside class="left-side sidebar-offcanvas">
                <!-- sidebar: style can be found in sidebar.less -->
                <section class="sidebar">
                    <!-- Sidebar user panel -->
                    <div class="user-panel">
                        <div class="pull-left image">
                        </div>
                        <div class="pull-left info">
                            <p>{{randomQuote.value}} <%= user.customerName %></p>
                        </div>
                    </div>
                    <!-- sidebar menu: : style can be found in sidebar.less -->
                    <ul class="sidebar-menu">
                        <li ng-class="{'active':page =='forms'}">
                            <a href="/forms">
                                <i class="fa fa-th-list"></i> <span>See Forms</span>
                            </a>
                        </li>
                        <li ng-class="{'active':page =='create'}">
                            <a href="/add">
                                <i class="fa fa-plus"></i> <span>Create Form</span>
                            </a>
                        </li>
                        <li ng-class="{'active':page =='import'}">
                            <a href="/import">
                                <i class="fa fa-upload"></i> <span>Import Form</span>
                            </a>
                        </li>
                        <li ng-class="{'active':page =='profile'}">
                            <a href="/profile">
                                <i class="fa fa-user"></i> <span>Profile</span>
                            </a>
                        </li>
                        <li ng-show="<%= user.account.role == 'admin' %> || <%= user.account.role == 'superadmin' %>"  ng-class="{'active':page =='users'}">
                            <a href="/userManagement">
                                <i class="fa fa-users"></i> <span>Manage Users</span>
                            </a>
                        </li>
                        <!-- <li ng-show="<%= user.account.role == 'admin' %> || <%= user.account.role == 'superadmin' %>"  ng-class="{'active':page =='categories'}">
                            <a href="/accountManagement">
                                <i class="fa fa-pencil"></i> <span>Manage Categories</span>
                            </a>
                        </li>  -->
                        <li>
                            <a href="#" target="_blank">
                                <i class="fa fa-question"></i> <span>Help</span>
                            </a>
                        </li>
                        <li>
                            <a href="mailto:it.sub@washpost.com?subject=SUB Question">
                                <i class="fa fa-envelope"></i> <span>Email Us</span>
                            </a>
                        </li>
                        <li>
                            <a href="/logout">
                                <span>Sign Out</span>
                            </a>
                        </li>
                    </ul>
                </section>
                <!-- /.sidebar -->
            </aside>
